# GitHub Pages repository URL used by deploy script (SSH or HTTPS)
DEPLOY_REPO="git@github.com:crackhopper/crackhopper.github.io.git"

# Branch that receives the published site (e.g. main or gh-pages)
DEPLOY_BRANCH="master"

# Folder name used as the deployment submodule (relative to repo root)
DEPLOY_DIR="repo_to_deploy"
